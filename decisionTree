import pandas as pd
from sklearn.model_selection import train_test_split

dataset = pd.read_csv("Social_Network_Ads.csv")

print(dataset)

x = dataset[["Age","EstimatedSalary"]]
y = dataset["Purchased"]

print(x.shape)
print(y.shape)


x_train,x_test,y_train,y_test = train_test_split(x,y,test_size=0.25)
