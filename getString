phone_number = {
    "1" : "one",
    "2" : "two",
    "3" : "three",
    "4" : "four",
    "5" : "five",
    "6" : "six",
    "7" : "seven",
    "8" : "eight",
    "9" : "nine",
    "10" : "ten"


}
number = input("what is your number? ")
new_number = " "
for item in number:
    new_number += phone_number.get(item,"!") + " "
print(new_number)






